    <script>
        function saveMoveMastery(id, isChecked) {
            const mastery = JSON.parse(localStorage.getItem('practix_desmos_mastery') || '{}');
            mastery[id] = isChecked;
            localStorage.setItem('practix_desmos_mastery', JSON.stringify(mastery));
            
            if (window.practixLog) {
                window.practixLog('desmos_mastery_toggle', { move_id: id, status: isChecked ? 'mastered' : 'unmastered' });
            }
        }

        function loadMoveMastery() {
            const mastery = JSON.parse(localStorage.getItem('practix_desmos_mastery') || '{}');
            document.querySelectorAll('.mastery-m-check').forEach(checkbox => {
                const id = checkbox.getAttribute('data-id');
                if (mastery[id]) {
                    checkbox.checked = true;
                }
            });
        }

        document.addEventListener('DOMContentLoaded', loadMoveMastery);
    </script>
